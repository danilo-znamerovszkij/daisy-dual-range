{
  "name": "daisy-dual-range",
  "version": "0.1.0",
  "description": "Framework-agnostic dual range (two-thumb) slider styled like DaisyUI, powered by noUiSlider.",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.iife.js",
  "jsdelivr": "./dist/index.iife.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "build:demo": "npm run build && rm -rf demo-site && mkdir -p demo-site && cp -R demo/* demo-site/ && cp -R dist demo-site/dist",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "prepublishOnly": "npm run build",
    "demo": "npm run build && serve . -l 4173"
  },
  "peerDependencies": {
    "nouislider": "^15.0.0"
  },
  "devDependencies": {
    "nouislider": "^15.0.0",
    "serve": "^14.2.5",
    "typescript": "^5.4.0",
    "vite": "^5.4.0"
  }
}
